<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="local/user_data_api/db" VERSION="20130407" COMMENT="XMLDB file for local user_data_api plugin"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
    <TABLES>
        <TABLE NAME="user_data_api" COMMENT="each record is a user_data_api">
            <FIELDS>
                <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
                <FIELD NAME="firstname" TYPE="text" NOTNULL="true" SEQUENCE="false"/>
                <FIELD NAME="lastname" TYPE="text" NOTNULL="true" SEQUENCE="false"/>
                <FIELD NAME="email" TYPE="text" NOTNULL="true" SEQUENCE="false"/>
                <FIELD NAME="phone" TYPE="text" NOTNULL="true" SEQUENCE="false"/>

            </FIELDS>
            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
            </KEYS>
        </TABLE>
    </TABLES>
</XMLDB>
